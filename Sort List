# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def sortList(self, head: Optional[ListNode]) -> Optional[ListNode]:
        list1=head
        ans=[]
        while list1:
            ans.append(list1.val)
            list1=list1.next
        ans=sorted(ans)
        list1=head
        for i in range(len(ans)):
            list1.val=ans[i]
            list1=list1.next
        return head
